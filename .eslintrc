{
  "rules": {
    "quotes": [ 2, "single" ],
    "semi": [ 2, "always" ],
    "no-unused-vars": [ 2, { "vars": "all", "args": "none" } ],
    "no-console": [ 0 ]
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  }
}
