{
  "name": "bicycle-touring-companion",
  "version": "0.3.0",
  "description": "Bicycle Touring Companion",
  "scripts": {
    "build": "browserify src/js/app.js -o www/bundle.js",
    "debug:android": "cordova build  android",
    "dev": "shell-exec --colored-output \"npm run watch\" \"npm run livereload\"",
    "doc": "docco src/js/*.js src/js/*/*.js -o docs/generated",
    "format": "esformatter --diff \"?(src|test)/js/**/*.js\"",
    "format:replace": "esformatter -i \"?(src|test)/js/**/*.js\"",
    "lint": "eslint \"?(src|test)/js/**/*.js\"",
    "livereload": "livereload \"platforms/browser/www/\"",
    "postbuild": "cp -r src/index.html src/img www/",
    "postdebug:android": "cp \"./platforms/android/build/outputs/apk/android-debug.apk\" \"./bicycle-touring-companion-debug.apk\"",
    "postinstall": "cordova prepare",
    "postrelease:android": "cp \"./platforms/android/build/outputs/apk/android-release-unsigned.apk\" \"./bicycle-touring-companion.apk\"",
    "prestart:android": "npm run build",
    "prestart:browser": "npm run build",
    "release:android": "cordova build android --release",
    "release:browser": "cordova build browser",
    "start:android": "cordova run android",
    "start:browser": "cordova serve 8000",
    "test": "karma start",
    "watch": "nodemon -e js,html,css --watch \"src\" --exec \"npm run start:browser\""
  },
  "browserify": {
    "transform": [
      "babelify",
      "browserify-css",
      "envify",
      "config-browserify"
    ]
  },
  "browserify-css": {
    "autoInject": true,
    "minify": false,
    "rootDir": "./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bikelomatic-complexity/btc-app.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bikelomatic-complexity/btc-app/issues"
  },
  "homepage": "https://github.com/bikelomatic-complexity/btc-app#readme",
  "dependencies": {
    "blob-util": "^1.2.0",
    "btc-models": "^0.4.2",
    "classnames": "^2.2.3",
    "docuri": "^4.2.1",
    "history": "^1.17.0",
    "immutable": "^3.7.6",
    "leaflet": "^0.7.7",
    "lodash": "^4.5.1",
    "material-ui": "^0.14.4",
    "moment-timezone": "^0.5.3",
    "ngeohash": "^0.6.0",
    "pouchdb": "^5.1.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-hammerjs": "^0.4.2",
    "react-leaflet": "^0.10.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.3",
    "superagent": "^1.7.2",
    "to-id": "^1.0.5",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-css": "^0.9.0",
    "cash-cp": "^0.1.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "config": "^1.19.0",
    "config-browserify": "^1.0.5",
    "cordova": "^6.0.0",
    "docco": "^0.7.0",
    "envify": "^3.4.0",
    "es5-shim": "^4.5.7",
    "es6-shim": "^0.34.4",
    "esdoc": "^0.4.1",
    "esformatter": "^0.9.2",
    "esformatter-ignore": "^0.1.3",
    "esformatter-jsx": "^4.1.4",
    "esformatter-line-endings": "^1.0.1",
    "eslint": "^2.2.0",
    "eslint-plugin-react": "^4.1.0",
    "istanbul": "^0.4.2",
    "js-yaml": "^3.5.3",
    "karma": "^0.13.21",
    "karma-browserify": "^5.0.1",
    "karma-es6-shim": "^0.2.3",
    "karma-jquery": "^0.1.0",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^1.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "livereload": "^0.4.0",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "phantomjs-prebuilt": "^2.1.4",
    "react-addons-test-utils": "^0.14.7",
    "redux-devtools": "^3.1.1",
    "shell-executor": "^0.3.2",
    "sinon": "^1.17.3",
    "skin-deep": "https://github.com/bikelomatic-complexity/skin-deep"
  }
}
